# IXFI Contracts Environment Variables
# Copy this file to .env and fill in your actual values

# RPC URL for CrossFi testnet
RPC_URL=https://rpc.testnet.ms

# Your private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# IXFI token address (update after deployment)
IXFI_ADDRESS=0x...

# DIA Oracle address (update after deployment)  
DIA_ORACLE_ADDRESS=0x...
